<%= form_with(model: @proyects, local: true) do |form| %>
    <div class="form-group">
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div class="form-group">
        <%= form.label :description %>
        <%= form.text_field :description %>
    </div>
    <div class="form-group">
        <%= form.label :state %>
        <%= form.select :state, options_for_select(Proyect.states.keys) %>
    </div>
    <div class="form-group">
        <%= form.label :initial_date %>
        <%= form.date_field :initial_date %>
    </div>
    <div class="form-group">
        <%= form.label :finish_date %>
        <%= form.date_field :finish_date %>
    </div>

    <%= form.submit "Crear Proyecto"%>
<% end %>   